<launch>
   <arg name="battery_runtime" default="60"/> 

   <node pkg="pi_kobuki_bringup" name="battery_simulator" type="battery_simulator.py" output="screen" clear_params="true">
     <param name="rate" value="5" />
     <param name="battery_runtime" value="$(arg battery_runtime)" />
   </node>
</launch>
